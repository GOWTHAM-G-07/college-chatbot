{
  "routes": [
    {
      "src": "^/$",
      "dest": "/frontend/login.html"
    },
    {
      "src": "^/chat$",
      "dest": "/frontend/chat.html"
    },
    {
      "src": "^/admin$",
      "dest": "/frontend/admin.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/frontend/$1"
    }
  ]
}
